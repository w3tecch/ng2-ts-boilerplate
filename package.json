{
  "name": "ng2-ts-boilerplate",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "start": "npm run server:dev",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --progress --profile --colors --display-error-details --inline --content-base src/ --env",
    "server:prod": "./node_modules/.bin/http-server dist --cors",
    "server:rest": "./node_modules/.bin/json-server --watch db.json --port 3004",
    "build": "npm run build:dev",
    "build:dev": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env",
    "build:prod": "./node_modules/.bin/webpack --config webpack.build.js --progress --profile --colors --display-error-details --display-cached --env prod",
    "test": "./node_modules/.bin/karma start",
    "test:live": "./node_modules/.bin/karma start --auto-watch --no-single-run",
    "webdriver": "./node_modules/protractor/bin/webdriver-manager update",
    "e2e": "./node_modules/.bin/protractor",
    "e2e:live": "npm run e2e -- --elementExplorer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w3tecch/ng2-ts-boilerplate"
  },
  "keywords": [
    "Angular2",
    "ng2-ts-boilerplate"
  ],
  "author": "David Weber <david.weber@w3tec.ch>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/w3tecch/ng2-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/w3tecch/ng2-ts-boilerplate#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "jquery": "^3.0.0",
    "lodash": "^4.13.1",
    "material-design-icons": "2.2.1",
    "materialize-css": "^0.97.6",
    "moment": "^2.13.0",
    "ng2-translate": "^2.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "assert": "^1.4.0",
    "awesome-typescript-loader": "^1.0.0",
    "bootstrap-sass": "^3.3.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chalk": "^1.1.3",
    "codelyzer": "0.0.23",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.0.0",
    "gulp-karma": "0.0.5",
    "gulp-task-listing": "^1.0.1",
    "gulp-typedoc": "^2.0.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.21.0",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.14",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.0.0",
    "karma-growl-reporter": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor2": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "lodash.merge": "^4.4.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.2.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.1",
    "typescript": "1.8.10",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.1.2",
    "yargs": "^4.7.1"
  }
}
